# .env file for Docker Compose

# Secret Key
SECRET_KEY=clinique2025_secure_key_change_in_production

# Database Configuration
DATABASE_URI=sqlite:///instance/base.db

# Docker Mode
USE_DOCKER=true

# Service URLs (used by containers)
AUTH_URL=http://auth-service:5009
DOCTORS_URL=http://doctors-service:5000
PATIENTS_URL=http://patients-backend:5001
RDV_URL=http://rdv-backend:5005

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=False

# Node Environment
NODE_ENV=production